<h1>Log In</h1>
<% if flash[:notice] %>
<p><%= flash[:notice] %></p>
<% end %>
<% if flash[:error_message]%>
<p><%= flash[:error_message] %></p>
<% end %>
<%= form_with(url: session_path) do %>
    <%=label_tag :email %>
    <%=text_field_tag :email %>
    <%=label_tag :password %>
    <%=password_field_tag :password %>
    <%=submit_tag "Log in" %>
<% end %>

<%= link_to 'Home', root_path, class: "button"%>
<%= link_to "Sign Up", new_user_path, class: "button" %> 