<h1>Log In</h1>


<% if flash[:error_message]%>
<p><%= flash[:error_message] %></p>
<% end %>
<%= form_with(url: session_path) do %>
    <%=label_tag :email %>
    <%=text_field_tag :email %>
    <%=label_tag :password %>
    <%=password_field_tag :password %>
    <%=submit_tag "Log in" %>
<% end %>